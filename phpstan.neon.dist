includes:
  - vendor/larastan/larastan/extension.neon

parameters:
  level: 6
  paths:
    - app
  tmpDir: storage/framework/cache/phpstan
  reportUnmatchedIgnoredErrors: true
  ignoreErrors:
    -
      message: '#^Class App\\Models\\DevOverrideLog uses generic trait Illuminate\\Database\\Eloquent\\Factories\\HasFactory but does not specify its types: TFactory$#'
      path: app/Models/DevOverrideLog.php